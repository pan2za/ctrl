
[program:glance-api]
command=/usr/bin/python /usr/bin/glance-api
user=glance
redirect_stderr=true
stdout_logfile=/var/log/contrail/glance-api-supervisor-stdout.log
stderr_logfile=/var/log/contrail/glance-api-supervisor-stderr.log
priority=440
autostart=true
killasgroup=true
stopasgroup=true
stopsignal=KILL
exitcodes=0
