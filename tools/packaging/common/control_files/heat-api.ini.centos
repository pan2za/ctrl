
[program:heat-api]
command=/usr/bin/python /usr/bin/heat-api
user=heat
redirect_stderr=true
stdout_logfile=/var/log/contrail/heat-api-supervisor-stdout.log
stderr_logfile=/var/log/contrail/heat-api-supervisor-stderr.log
priority=440
autostart=true
killasgroup=true
stopasgroup=true
stopsignal=KILL
exitcodes=0
