
[program:heat-engine]
command=/usr/bin/python /usr/bin/heat-engine
user=heat
redirect_stderr=true
stdout_logfile=/var/log/contrail/heat-engine-supervisor-stdout.log
stderr_logfile=/var/log/contrail/heat-engine-supervisor-stderr.log
priority=440
autostart=true
killasgroup=true
stopasgroup=true
stopsignal=KILL
exitcodes=0
